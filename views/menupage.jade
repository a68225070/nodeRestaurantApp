doctype
html(lang='en')
head
    meta(charset='UTF-8')
    title Menu
    link(rel='stylesheet', type='text/css', href='public/stylesheets/bootstrap.css')
    link(rel='icon', type='image/ico', href='public/images/cross.ico')
  body
    .navbar.navbar-default
      .navbar-header
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-responsive-collapse')
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='#') Feed.me
      .navbar-collapse.collapse.navbar-responsive-collapse
        ul.nav.navbar-nav
          li
            a(href='#') Dashboard
          li
            a(href='#') Voting
          li.active
            a(href='#') Order
          li
            a(href='#') History
          li
            a(href='form.html') User Management
          li
            a(href='#') Restaurant Management
          li#admin
        ul.nav.navbar-nav.navbar-right
          li
            a#signOut(href='#') Sign Out
    .row
      .list-group.col-md-6.col-md-offset-3
        each item in data
          a.list-group-item.disabled(href='#') #{item.name}
          each i in item.items
            a.list-group-item(href='#') #{i.itemName}
                span.pull-right.price $#{i.price}
    .row
      textarea.col-md-6.col-md-offset-3(input-type='text', rows='3', placeholder='Special Requests')
    button.btn.btn-success.col-md-offset-8(type='button', style='margin-top: 20px') Submit
    img#icon(src='public/images/cross.ico')
    #wrap
      #main.container.clear-top
    footer.footer
    script(src='http://code.jquery.com/jquery.js')
    script(src='http://maxcdn.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js')
    script(src='js/authlogin.js')
